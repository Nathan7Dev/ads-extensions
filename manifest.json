{
  "manifest_version": 3,
  "name": "BletNinjaClose",
  "description": "Extensão que fecha anúncios e popups automaticamente, seguindo a Habblet Etiqueta para uma navegação mais limpa.",
  "version": "1.1",
  "author": "Nathan,",
  "host_permissions": [
    "*://habblet.city/hotel*",
    "*://www.habblet.city/hotel/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://habblet.city/hotel*",
        "*://www.habblet.city/hotel/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle",
      "world": "MAIN"
    }
  ],
  "icons": {
    "16": "sharingan_16.png",
    "48": "sharingan_48.png",
    "128": "sharingan_128.png"
  },
  "action": {
    "default_title": "BletNinjaClose"
  }
}